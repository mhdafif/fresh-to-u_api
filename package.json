{
  "name": "@fresh-to-u/api",
  "version": "1.0.0",
  "main": "dist/index.js",
  "scripts": {
    "dev": "nodemon",
    "build": "tsc",
    "start": "node dist/index.js",
    "lint": "eslint src --ext .ts",
    "lint:fix": "eslint src --ext .ts --fix",
    "type-check": "tsc --noEmit",
    "prisma:generate": "prisma generate --schema src/prisma/schema.prisma",
    "prisma:migrate": "prisma migrate dev --schema src/prisma/schema.prisma",
    "prisma:studio": "prisma studio --schema src/prisma/schema.prisma",
    "prisma:push": "prisma db push --schema src/prisma/schema.prisma",
    "prisma:pull": "prisma db pull --schema src/prisma/schema.prisma",
    "prisma:reset": "prisma migrate reset --schema src/prisma/schema.prisma",
    "prisma:validate": "prisma validate --schema src/prisma/schema.prisma",
    "prisma:format": "prisma format --schema src/prisma/schema.prisma"
  },
  "keywords": [
    "food",
    "ai",
    "vision",
    "api"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@ai-sdk/openai": "^2.0.80",
    "@aws-sdk/client-s3": "^3.946.0",
    "@aws-sdk/s3-request-presigner": "^3.946.0",
    "@prisma/adapter-pg": "^7.1.0",
    "@prisma/client": "^7.1.0",
    "ai": "^5.0.108",
    "bcryptjs": "^3.0.3",
    "better-auth": "^1.4.5",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "dotenv": "^17.2.3",
    "express": "^4.21.2",
    "express-rate-limit": "^8.2.1",
    "helmet": "^8.1.0",
    "jsonwebtoken": "^9.0.3",
    "morgan": "^1.10.1",
    "pg": "^8.16.3",
    "prisma": "^7.1.0",
    "react": "19.2.1",
    "uuid": "^13.0.0",
    "zod": "^4.1.13"
  },
  "devDependencies": {
    "@types/cookie-parser": "^1.4.10",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.6",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/morgan": "^1.9.10",
    "@types/node": "^20.19.25",
    "eslint": "^9.39.1",
    "nodemon": "^3.1.11",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3"
  }
}
